{
  "name": "precisely-web",
  "description": "",
  "private": true,
  "scripts": {
    "test": "jest --no-cache --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.graphql --collectCoverageFrom=!src/interfaces.ts",
    "start": "AWS_PROFILE=dev-profile-precisely ./node_modules/.bin/env-cmd ../config/security.env.local ./node_modules/.bin/serverless offline start",
    "postinstall": "env-cmd ../config/security.env.local ./node_modules/.bin/serverless dynamodb install",
    "setup:database": "serverless invoke local -f setupDatabase",
    "seed:create:dev": "env-cmd ../config/security.env.local ts-node -O '{\"module\": \"commonjs\"}' src/seed-data/scripts/createFakeData.ts",
    "seed:create:stage": "env-cmd ../config/security.env.stage ts-node -O '{\"module\": \"commonjs\"}' src/seed-data/scripts/createFakeData.ts",
    "seed:local": "DB=local env-cmd ../config/security.env.local ts-node -O '{\"module\": \"commonjs\"}' src/seed-data/scripts/seedData.ts",
    "seed:dev": "sls decrypt | env-cmd ../config/security.env.local xargs ts-node -O '{\"module\": \"commonjs\"}' src/seed-data/scripts/seedData.ts",
    "seed:stage": "sls decrypt -s stage | env-cmd ../config/security.env.stage xargs ts-node -O '{\"module\": \"commonjs\"}' src/seed-data/scripts/seedData.ts",
    "deploy:dev": "env-cmd ../config/security.env.local serverless deploy -s dev",
    "deploy:stage": "env-cmd ../config/security.env.stage serverless deploy -s stage",
    "deploy:prod": "env-cmd ../config/security.env.prod ./node_modules/.bin/serverless deploy -s prod",
    "tslint": "tslint 'src/*.ts' 'src/**/*.ts'",
    "eslint": "eslint src"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true,
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "0.0.9",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "camelcase-keys": "4.2.0",
    "dynogels": "9.0.0",
    "dynogels-promisified": "1.0.4",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.3",
    "graphql-tag": "2.6.1",
    "graphql-tools": "2.23.1",
    "jest-transform-graphql": "2.1.0",
    "lodash.snakecase": "4.1.1",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "sequelize": "4.35.2",
    "serverless-dynamodb-client": "0.0.2",
    "serverless-plugin-scripts": "1.0.2",
    "winston": "3.0.0-rc2"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.27",
    "@types/bluebird": "3.5.20",
    "@types/dynogels": "8.0.0",
    "@types/faker": "4.1.2",
    "@types/graphql": "0.12.1",
    "@types/jest": "21.1.4",
    "@types/node": "9.3.0",
    "@types/sequelize": "4.27.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "body-parser": "1.15.2",
    "cors": "2.8.4",
    "env-cmd": "5.0.0",
    "eslint": "4.17.0",
    "faker": "4.1.0",
    "imports-loader": "0.7.1",
    "jest": "21.2.1",
    "jest-plugin-context": "^2.7.0",
    "jsonfile": "4.0.0",
    "sequelize-mock": "0.10.2",
    "serverless": "1.24.1",
    "serverless-dynamodb-local": "0.2.26",
    "serverless-kms-secrets": "1.0.2",
    "serverless-offline": "3.17.0",
    "serverless-webpack": "4.1.0",
    "ts-jest": "22.0.1",
    "ts-loader": "3.3.0",
    "tslint": "5.7.0",
    "typescript": "2.6.2",
    "unroll": "1.5.0",
    "webpack": "3.8.1",
    "webpack-graphql-loader": "0.1.0",
    "webpack-node-externals": "1.6.0"
  }
}

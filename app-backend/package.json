{
  "name": "precisely-web",
  "description": "",
  "private": true,
  "scripts": {
    "console": "TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" ts-node -r tsconfig-paths/register -c \"require('ts-node').register()\"",
    "shell": "scripts/sls shell",
    "log": "scripts/log",
    "deploy": "yarn sls deploy",
    "test": "jest --no-cache --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.graphql --collectCoverageFrom=!src/interfaces.ts",
    "offline": "scripts/offline",
    "ts-node": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register",
    "sls": "./scripts/sls",
    "tslint": "tslint 'src/*.ts' 'src/**/*.ts'",
    "eslint": "eslint src",
    "dynogels:tables": "ts-node -r tsconfig-paths/register -O '{\"module\": \"commonjs\"}' scripts/dynogels-tables.ts"
  },
  "jest": {
    "collectCoverage": true,
    "cover ageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true,
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "@aneilbaboo/dynogels": "^9.0.0",
    "@aneilbaboo/dynogels-promisified": "^1.0.5",
    "@aneilbaboo/winston-cloudwatch": "^1.13.0",
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "^0.1.4",
    "auth0-lock": "^11.5.2",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.3",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "2.23.1",
    "jest-transform-graphql": "2.1.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "jwks-rsa": "^1.2.1",
    "jwks-rsa-promisified": "^1.0.2",
    "lodash.camelcase": "^4.3.0",
    "logform": "1.5.0",
    "sequelize": "4.35.2",
    "slugify": "^1.2.9",
    "winston": "3.0.0-rc2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/faker": "4.1.2",
    "@types/graphql": "0.12.1",
    "@types/jest": "21.1.4",
    "@types/joi": "^13.0.7",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/jsonwebtoken-promisified": "^1.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^9.6.4",
    "@types/sequelize": "4.27.4",
    "body-parser": "1.15.2",
    "cors": "2.8.4",
    "dynamodb-localhost": "^0.0.5",
    "env-cmd": "5.0.0",
    "eslint": "4.17.0",
    "faker": "4.1.0",
    "imports-loader": "0.7.1",
    "jest": "^22.4.3",
    "jest-plugin-context": "^2.7.0",
    "jsonfile": "4.0.0",
    "sequelize-mock": "0.10.2",
    "serverless": "1.24.1",
    "serverless-api-stage": "^1.3.0",
    "serverless-domain-manager": "^2.3.6",
    "serverless-dotenv-plugin": "^1.0.2",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "^0.2.28",
    "serverless-enable-api-logs": "^2.0.0",
    "serverless-offline": "^3.20.3",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-secrets": "https://github.com/aneilbaboo/serverless-secrets.git#aneil/fix-process.env._HANDLER",
    "serverless-shell": "^0.4.1",
    "serverless-webpack": "4.1.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^4.1.0",
    "tsconfig-paths": "^3.2.0",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "5.7.0",
    "typescript": "^2.8.1",
    "unroll": "1.5.0",
    "webpack": "^4.5.0",
    "webpack-graphql-loader": "0.1.0",
    "webpack-node-externals": "1.6.0"
  }
}

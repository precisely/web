{
  "name": "precisely-web-backend",
  "description": "",
  "private": true,
  "scripts": {
    "build": "yarn sls webpack",
    "console": "yarn sls console",
    "deploy:new": "yarn sls create_domain && yarn deploy && tput setaf 6 && echo \"New domains may take up to 40 minutes to be initialized.\n\" && tput setaf sgr0",
    "deploy:delete": "scripts/guard-prod && yarn sls remove && yarn sls delete_domain",
    "deploy": "yarn sls deploy",
    "dynamodb": "yarn sls dynamodb start",
    "dynamodb:test:offline": "ENV=test-offline yarn sls dynamodb start",
    "dynamodb:admin": "dynamodb-admin",
    "env": "../config/withenv",
    "log": "scripts/log",
    "offline": "NODE_OPTIONS=--max-old-space-size=4096 yarn sls offline start",
    "offline:test": "ENV=test-offline NODE_OPTIONS=--max-old-space-size=4096 yarn sls offline start",
    "sls": "yarn env sls",
    "tslint": "tslint 'src/*.ts' 'src/**/*.ts'",
    "test": "ENV=${ENV:-test-offline} yarn env jest --runInBand",
    "wsd": "scripts/wsd"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "^.+\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.(test|spec)\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "graphql"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "@aneilbaboo/dynogels": "^9.0.0",
    "@aneilbaboo/dynogels-promisified": "^1.0.5",
    "@aneilbaboo/serverless-dynamodb-local": "^0.2.33",
    "@aneilbaboo/winston-cloudwatch": "^1.13.0",
    "@types/graphql-type-json": "^0.1.2",
    "accesscontrol-plus": "^1.0.1",
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "^0.1.4",
    "auth0-lock": "^11.5.2",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "batch-promises": "^0.0.3",
    "bluebird": "3.5.1",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.3",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "2.23.1",
    "graphql-type-json": "^0.2.1",
    "interleave26": "^1.0.0",
    "jest-transform-graphql": "2.1.0",
    "joi": "^13.4.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "jwks-rsa": "^1.2.1",
    "jwks-rsa-promisified": "^1.0.2",
    "lodash": "^4.17.10",
    "logform": "1.5.0",
    "markdown-it": "^8.4.1",
    "sequelize": "4.35.2",
    "seqvarnomjs": "git+ssh://git@github.com/precisely/seqvarnomjs.git#1.2.0",
    "slugify": "^1.3.0",
    "smart-report": "git+ssh://git@github.com/precisely/smart-report.git#1.1.6",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/faker": "4.1.2",
    "@types/graphql": "^0.13.3",
    "@types/jest": "21.1.4",
    "@types/jest-in-case": "^1.0.0",
    "@types/joi": "^13.3.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/jsonwebtoken-promisified": "^1.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^9.6.4",
    "@types/sequelize": "4.27.4",
    "body-parser": "^1.18.3",
    "cors": "2.8.4",
    "graphql-import": "^0.6.0",
    "jest": "^23.4.1",
    "jest-in-case": "^1.0.2",
    "jest-plugin-context": "^2.7.0",
    "jsonfile": "4.0.0",
    "serverless": "^1.28.0",
    "serverless-api-stage": "^1.3.0",
    "serverless-domain-manager": "^2.3.6",
    "serverless-offline": "^3.25.4",
    "serverless-plugin-bind-deployment-id": "^1.0.1",
    "serverless-plugin-scripts": "https://github.com/precisely/serverless-plugin-scripts/releases/download/v1.0.2precisely1/serverless-plugin-scripts-v1.0.2.tgz",
    "serverless-pseudo-parameters": "^1.6.0",
    "serverless-webpack": "^5.2.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^4.1.0",
    "tsconfig-paths": "^3.4.0",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "5.7.0",
    "typescript": "^2.8.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-graphql-loader": "^1.0.0",
    "webpack-node-externals": "1.6.0",
    "websequencediagrams": "^0.1.1"
  }
}

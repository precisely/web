{
  "name": "precisely-web",
  "description": "",
  "private": true,
  "scripts": {
    "test": "jest --no-cache --coverage",
    "start": "AWS_PROFILE=dev-profile-precisely ./node_modules/.bin/env-cmd ../config/security.env.local serverless offline start",
    "postinstall": "serverless dynamodb install",
    "deploy-prod": "serverless --stage=production deploy",
    "deploy-stage": "serverless --stage=stage deploy"
  },
  "jest": {
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "0.0.9",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "graphql": "0.10.5",
    "graphql-playground-middleware-lambda": "1.2.0",
    "graphql-tag": "2.6.1",
    "graphql-tools": "2.7.2",
    "jest-transform-graphql": "2.1.0",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "sequelize": "4.27.0",
    "serverless-dynamodb-client": "0.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.27",
    "@types/bluebird": "3.5.20",
    "@types/graphql": "0.12.1",
    "@types/node": "9.3.0",
    "@types/sequelize": "4.27.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "body-parser": "1.15.2",
    "cors": "2.8.4",
    "env-cmd": "5.0.0",
    "faker": "4.1.0",
    "imports-loader": "0.7.1",
    "jest": "21.2.1",
    "jsonfile": "4.0.0",
    "serverless-dynamodb-local": "0.2.26",
    "serverless-kms-secrets": "1.0.2",
    "serverless-offline": "3.15.3",
    "serverless-webpack": "4.1.0",
    "sequelize-mock": "0.10.2",
    "ts-jest": "22.0.1",
    "ts-loader": "3.3.0",
    "typescript": "2.6.2",
    "unroll": "1.5.0",

{
  "name": "precisely-web-backend",
  "description": "",
  "private": true,
  "scripts": {
    "console": "yarn sls console",
    "deploy": "yarn sls deploy",
    "dynamodb": "yarn sls dynamodb start",
    "dynamodb:test:offline": "ENV=test-offline yarn sls dynamodb start",
    "dynamodb:admin": "dynamodb-admin",
    "env": "../config/withenv",
    "eslint": "eslint src",
    "log": "scripts/log",
    "offline": "NODE_OPTIONS=--max-old-space-size=4096 yarn sls offline start",
    "sls": "yarn env sls",
    "test:offline": "ENV=test-offline yarn sls test",
    "test:offline:watch": "ENV=test-offline yarn sls test:watch",
    "test:unit": "jest --testPathPattern '.*\\.unit\\.test\\.(js|ts)x?'",
    "testcov": "STAGE=test jest --no-cache --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.graphql",
    "tslint": "tslint 'src/*.ts' 'src/**/*.ts'",
    "wsd": "scripts/wsd"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testEnvironment": "node",
    "testRegex": ".*\\.(test|spec)\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "@aneilbaboo/dynogels": "^9.0.0",
    "@aneilbaboo/dynogels-promisified": "^1.0.5",
    "@aneilbaboo/serverless-dynamodb-local": "^0.2.33",
    "@aneilbaboo/winston-cloudwatch": "^1.13.0",
    "@types/graphql-type-json": "^0.1.2",
    "accesscontrol-plus": "^1.0.1",
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "^0.1.4",
    "auth0-lock": "^11.5.2",
    "aws-lambda": "^0.1.2",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.3",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "2.23.1",
    "graphql-type-json": "^0.2.1",
    "interleave26": "^1.0.0",
    "jest-transform-graphql": "2.1.0",
    "joi": "^13.4.0",
    "jsonwebtoken-promisified": "^1.0.3",
    "jwks-rsa": "^1.2.1",
    "jwks-rsa-promisified": "^1.0.2",
    "lodash.camelcase": "^4.3.0",
    "logform": "1.5.0",
    "markdown-it": "^8.4.1",
    "sequelize": "4.35.2",
    "seqvarnomjs": "git+ssh://git@github.com/precisely/seqvarnomjs#1.1.7",
    "slugify": "^1.3.0",
    "smart-report": "git+ssh://git@github.com/precisely/smart-report.git#1.1.3",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/faker": "4.1.2",
    "@types/graphql": "^0.13.3",
    "@types/jest": "21.1.4",
    "@types/joi": "^13.3.0",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/jsonwebtoken-promisified": "^1.0.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^9.6.4",
    "@types/sequelize": "4.27.4",
    "body-parser": "1.15.2",
    "cors": "2.8.4",
    "eslint": "4.17.0",
    "jest": "^22.4.4",
    "jest-plugin-context": "^2.7.0",
    "jsonfile": "4.0.0",
    "serverless": "1.27.0",
    "serverless-api-stage": "^1.3.0",
    "serverless-domain-manager": "^2.3.6",
    "serverless-offline": "^3.25.4",
    "serverless-plugin-scripts": "https://github.com/precisely/serverless-plugin-scripts/releases/download/v1.0.2precisely1/serverless-plugin-scripts-v1.0.2.tgz",
    "serverless-pseudo-parameters": "^1.6.0",
    "serverless-webpack": "4.1.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^4.1.0",
    "tsconfig-paths": "^3.4.0",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "5.7.0",
    "typescript": "^2.8.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-graphql-loader": "0.1.0",
    "webpack-node-externals": "1.6.0",
    "websequencediagrams": "^0.1.1"
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}

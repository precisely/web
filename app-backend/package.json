{
  "name": "precisely-web",
  "description": "",
  "private": true,
  "scripts": {
    "console": "TS_NODE_COMPILER_OPTIONS=\"{\\\"module\\\":\\\"commonjs\\\"}\" ts-node -r tsconfig-paths/register -c \"require('ts-node').register()\"",
    "test": "jest --no-cache --coverage --collectCoverageFrom=src/**/*.ts --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.graphql --collectCoverageFrom=!src/interfaces.ts",
    "start": "sls offline start",
    "dynamodb:install": "sls dynamodb install",
    "dynamodb:remove": "sls dynamodb remove",
    "dynamodb:start": "sls dynamodb start",
    "ts-node": "ts-node -O '{\"module\": \"commonjs\"}' -r tsconfig-paths/register",
    "deploy:dev": "SLS_DEBUG=* env-cmd ../config/dev.env sls deploy",
    "deploy": "sls deploy",
    "deploy:remove": "sls remove",
    "tslint": "tslint 'src/*.ts' 'src/**/*.ts'",
    "eslint": "eslint src",
    "dynogels:tables": "ts-node -r tsconfig-paths/register -O '{\"module\": \"commonjs\"}' scripts/dynogels-tables.ts"
  },
  "jest": {
    "collectCoverage": true,
    "cover ageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**"
    ],
    "transform": {
      "^.+\\.ts?$": "./node_modules/ts-jest/preprocessor.js",
      "\\.(gql|graphql)$": "jest-transform-graphql"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "mapCoverage": true,
    "setupFiles": [
      "jest-plugin-context/setup"
    ]
  },
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "dependencies": {
    "@aneilbaboo/dynogels": "^9.0.0",
    "@aneilbaboo/dynogels-promisified": "^1.0.5",
    "@types/jsonwebtoken": "^7.2.6",
    "apollo-server-lambda": "1.3.2",
    "apollo-tracing": "^0.1.4",
    "auth0-lock": "^11.5.2",
    "aws-amplify": "^0.2.14",
    "aws-sdk": "2.188.0",
    "babel-polyfill": "6.26.0",
    "bluebird": "3.5.1",
    "graphql": "0.13.2",
    "graphql-playground-middleware-lambda": "1.4.3",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "2.23.1",
    "jest-transform-graphql": "2.1.0",
    "jsonwebtoken": "^8.2.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "4.1.1",
    "pad": "^2.0.3",
    "pg": "7.4.1",
    "pg-hstore": "2.3.2",
    "sequelize": "4.35.2",
    "slugify": "^1.2.9",
    "winston": "3.0.0-rc2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.20",
    "@types/faker": "4.1.2",
    "@types/graphql": "0.12.1",
    "@types/jest": "21.1.4",
    "@types/joi": "^13.0.7",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^9.6.4",
    "@types/sequelize": "4.27.4",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "body-parser": "1.15.2",
    "cors": "2.8.4",
    "env-cmd": "5.0.0",
    "eslint": "4.17.0",
    "faker": "4.1.0",
    "imports-loader": "0.7.1",
    "jest": "21.2.1",
    "jest-plugin-context": "^2.7.0",
    "jsonfile": "4.0.0",
    "sequelize-mock": "0.10.2",
    "serverless": "1.24.1",
    "serverless-cloudformation-sub-variables": "^0.2.0",
    "serverless-dotenv-plugin": "^1.0.2",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "0.2.26",
    "serverless-kms-secrets": "^1.0.2",
    "serverless-offline": "3.17.0",
    "serverless-plugin-additional-stacks": "^1.1.0",
    "serverless-plugin-scripts": "^1.0.2",
    "serverless-webpack": "4.1.0",
    "ts-jest": "22.0.1",
    "ts-loader": "^4.1.0",
    "tsconfig-paths": "^3.2.0",
    "tsconfig-paths-webpack-plugin": "^3.0.3",
    "tslint": "5.7.0",
    "typescript": "^2.8.1",
    "unroll": "1.5.0",
    "webpack": "^4.5.0",
    "webpack-graphql-loader": "0.1.0",
    "webpack-node-externals": "1.6.0"
  }
}

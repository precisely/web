{
  "extends": [
    "tslint-react",
    "tslint-config-airbnb"
  ],
  "rules": {
    "align": [true, "parameters", "statements", "members", "elements"],
    "console": null,
    "import-name": null,
    "jsx-alignment": null,
    "max-line-length": 150,
    "no-boolean-literal-compare": null,
    "no-consecutive-blank-lines": [true, 2],
    "object-curly-spacing": null,
    "trailing-comma": "never",
    "variable-name": [true, "check-format", "allow-pascal-case", "ban-keywords"]
  }
}

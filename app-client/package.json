{
  "name": "precisely-web-client",
  "description": "",
  "main": "webpack.config.js",
  "author": "Precise.ly, Inc",
  "license": "ISC",
  "private": true,
  "scripts": {
    "build": "rm -frd build/* && yarn sls build",
    "console": "yarn sls console",
    "deploy": "yarn deploy:resources && yarn deploy:code",
    "deploy:resources": "yarn env sls deploy --force",
    "deploy:code": "yarn build && yarn deploy:codeToS3",
    "deploy:codeToS3": "yarn env bash -c 'sls client deploy -s $STAGE -r $REGION'",
    "env": "../config/withenv",
    "eslint": "eslint src",
    "sls": "yarn env sls",
    "start": "yarn sls static",
    "start:offline": "IS_OFFINE=true yarn sls static",
    "test": "jest --env=jsdom",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --env=jsdom --runInBand",
    "tslint": "tslint 'src/*.tsx' 'src/**/*.tsx' 'src/*.ts' 'src/**/*.ts'"
  },
  "jest": {
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/interfaces/**",
      "!src/__tests__/**",
      "!src/index.tsx",
      "!**/registerServiceWorker.ts",
      "!src/ambientInterfaces.d.ts"
    ],
    "coverageReporters": [
      "html",
      "text",
      "lcov",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock"
    ],
    "testRegex": "(/tests/.*-test|\\.(test|spec))\\.(ts|tsx)$",
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "parser": "typescript-eslint-parser",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ]
  },
  "dependencies": {
    "amazon-cognito-identity-js": "1.31.0",
    "apollo-boost": "0.1.7",
    "apollo-cache": "1.1.9",
    "apollo-cache-inmemory": "^1.2.8",
    "apollo-client": "^2.4.0",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "apollo-utilities": "1.0.13",
    "auth0-js": "^9.6.1",
    "autoprefixer": "7.1.6",
    "aws-sdk": "2.188.0",
    "axios": "0.17.1",
    "bluebird": "3.5.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "env-cmd": "7.0.0",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "graphql-anywhere": "4.1.11",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "2.29.0",
    "immutable": "3.8.2",
    "iscroll": "5.2.0",
    "iscroll-react": "1.0.3",
    "lodash": "^4.17.10",
    "markdown-components": "^2.0.0",
    "memory-streams": "0.1.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "radium": "0.19.6",
    "react": "^16.4.2",
    "react-apollo": "^2.1.11",
    "react-dev-utils": "4.0.1",
    "react-dom": "16.1.1",
    "react-fastclick": "3.0.2",
    "react-fontawesome": "1.6.1",
    "react-iscroll": "2.0.3",
    "react-loadable": "5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "react-scroll-parallax": "1.2.1",
    "react-select": "1.0.0-rc.10",
    "react-toastify": "3.2.2",
    "react-transition-group": "2.2.1",
    "reactstrap": "^6.4.0",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "reselect": "3.0.1",
    "source-map-loader": "0.2.1",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "ts-loader": "2.3.7",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-loader": "3.5.3",
    "tslint-react": "3.2.0",
    "types": "^0.1.1",
    "typescript": "^3.0.1",
    "url-loader": "0.5.9",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.7.1",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/auth0-js": "^8.11.2",
    "@types/bluebird": "^3.5.23",
    "@types/enzyme": "3.1.1",
    "@types/enzyme-adapter-react-16": "1.0.0",
    "@types/graphql": "0.12.3",
    "@types/jest": "22.0.1",
    "@types/lodash": "^4.14.110",
    "@types/node": "8.0.46",
    "@types/radium": "0.18.23",
    "@types/react": "^16.4.11",
    "@types/react-dom": "16.0.3",
    "@types/react-fontawesome": "1.6.1",
    "@types/react-loadable": "5.3.0",
    "@types/react-redux": "5.0.11",
    "@types/react-router-dom": "4.2.0",
    "@types/react-router-redux": "5.0.10",
    "@types/react-test-renderer": "^16.0.1",
    "@types/react-transition-group": "2.0.6",
    "@types/reactstrap": "5.0.12",
    "@types/redux-logger": "3.0.5",
    "apollo-mocknetworkinterface": "2.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "7.2.3",
    "babel-jest": "^23.4.2",
    "copy-webpack-plugin": "^4.5.2",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "4.17.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-flowtype": "2.42.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.6.1",
    "jest": "^23.5.0",
    "jest-localstorage-mock": "^2.2.0",
    "jest-plugin-context": "^2.7.0",
    "react-test-renderer": "16.0.0",
    "serverless": "^1.27.3",
    "serverless-finch": "1.3.0",
    "serverless-plugin-scripts": "https://github.com/precisely/serverless-plugin-scripts/releases/download/v1.0.2precisely1/serverless-plugin-scripts-v1.0.2.tgz",
    "ts-jest": "^23.1.4",
    "ts-node": "^3.2.0",
    "tsconfig-paths": "^3.4.0",
    "tslint": "^5.11.0",
    "typescript-eslint-parser": "^18.0.0",
    "unroll": "1.5.0"
  },
  "resolutions": {
    "eslint-scope": "3.7.1"
  }
}
